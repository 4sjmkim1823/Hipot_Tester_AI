                <!-- ToggleButton -->
                <ResourceDictionary Source="/Resource/ToggleButton/ToggleButtonDictionary1.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
